<svg data-name="Layer 1" id="white-mask" viewbox="0 0 421.8 89.5" xmlns="http://www.w3.org/2000/svg"><path d="M421.8,57.5V28.23h0A28.48,28.48,0,0,1,377,52.32a289.66,289.66,0,0,0-332.1,0A28.5,28.5,0,0,1,0,28.23H0V57.5H0v32H421.8v-32Z" style="fill:#fff"></path></svg>
<script>
			jQuery(document).ready(function( $ ) {
				var allPanels = $('.accordion > dd').hide();
				var allTitles = $('.accordion > dt');
				var allPrimary = $('.primary-image > img');
				var allBackground = $('.background-image > img');

				$('.accordion > dt:first-of-type').addClass('active');
				$('.accordion > dd:first-of-type').show().addClass('active');


				$('#primary0 img').attr("src","https://res.cloudinary.com/everfi/images/f_auto,q_auto/v1615775977/b4b-1-alt/b4b-1-alt.png?_i=AA");
				$('#primary0 img').addClass('active');
				$('#background0 img').addClass('active');

				$('.accordion > dt > a').click(function() {
					$this = $(this);
					$target =  $this.parent();

					if(!$target.hasClass('active')){
						allTitles.removeClass('active');
						allPanels.removeClass('active').slideUp();

						$target.addClass('active');
						$target.next().addClass('active').slideDown();

						allPrimary.removeClass('active');
						allBackground.removeClass('active');

						var primary = $this.attr('data-primary') + ' img';
						var background = $this.attr('data-background') + ' img';

						$(primary).addClass('active');
						$(background).addClass('active');
					}

					return false;
				});

			});
		</script>
