<script>
//There is a hidden field labeled "state"
//This function populates the hidden field with the selected state name. Located in footer.php
jQuery( document ).ready(function() {
    jQuery('.word-force-research-study-form .state-field .gfield_select').change(function(){
        var state = (jQuery(".word-force-research-study-form .state-field .gfield_select option:selected").text());
        jQuery('#input_131_11').attr('value', state );
    });
});
</script>
<!-- Custom JavaScript to hide raw HubSpot shortcode text on blog pages-->
<script>
  document.addEventListener("DOMContentLoaded", function () {
    // Select all elements with class 'form-box' that might contain the unwanted text
    const elements = document.querySelectorAll(".form-box");

    elements.forEach(function (el) {
      // Filter text nodes inside the element that contain the HubSpot shortcode string
      const textNodes = Array.from(el.childNodes).filter(node =>
        node.nodeType === Node.TEXT_NODE &&
        node.nodeValue.includes('[hubspot type="form" portal="49456359"')
      );

      // Clear the matched text nodes to hide the raw shortcode from view
      textNodes.forEach(node => {
        node.nodeValue = ''; // You could also use node.remove() in modern browsers
      });
    });
  });
</script>
